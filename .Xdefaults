! Xft settings ---------------------------------------------------------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:  rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull
Xft.lcdfilter:  lcddefault

XTerm*termName: xterm-256color

urxvt*termName: rxvt-256color
!urxvt*transparent: true
!urxvt*shading: 50
!urxvt*depth: 32
!urxvt*tintColor: #000000
!urxvt*blurRadius: 15
urxvt*saveLines: 12000
urxvt*cursorBlink: false
urxvt*underlineColor: #dd0000
urxvt*scrollBar: false
urxvt*scrollBar_right: true
urxvt*scrollstyle: plain
urxvt*scrollColor: #0a0a0a
urxvt*borderLess:           false
urxvt*inheritPixmap:	    false
urxvt.perl-lib:             /usr/lib/urxvt/perl
urxvt.perl-ext-common:      default,matcher,url-select,keyboard-select
urxvt.urlLauncher: /usr/bin/firefox
urxvt.url-select.launcher: /usr/bin/firefox
urxvt.matcher.button: 1 
urxvt.keysym.M-u:           perl:url-select:select_next
urxvt.keysym.M-Escape:      perl:keyboard-select:activate
urxvt.keysym.M-s:           perl:keyboard-select:search
urxvt*keysym.S-Home:		\033[1;2H
urxvt*keysym.S-End:		\033[1;2F
urxvt*keysym.F1:		\033OP
urxvt*keysym.F2:		\033OQ
urxvt*keysym.F3:		\033OR
urxvt*keysym.F4:		\033OS


